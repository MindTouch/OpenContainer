{
  "name": "modethirteen/opencontainer",
  "description": "A dependency injection container for PHP, please enjoy responsibly",
  "type": "library",
  "license": "Apache-2.0",
  "authors" : [
    {
      "name": "James Andrew Vaughn",
      "email": "jamesvaughn@modethirteen.com",
      "role": "maintainer"
    }
  ],
  "support": {
    "issues": "https://github.com/modethirteen/opencontainer/issues"
  },
  "require": {
    "php": "~7.4.12",
    "psr/container": "~1.0.0",
    "ocramius/proxy-manager": "~2.10.0"
  },
  "require-dev": {
    "phpstan/phpstan": "~0.12.57",
    "phpunit/phpunit": "~9.4.3"
  },
  "autoload": {
    "psr-4": {
      "modethirteen\\OpenContainer\\": ["src/"]
    },
    "classmap": [
      "src/Exceptions.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "modethirteen\\OpenContainer\\Tests\\": ["tests/"]
    }
  }
}
